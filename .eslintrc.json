{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [ 
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-mixed-spaces-and-tabs":0,
        "react/no-unescaped-entities":0,
        "react/prop-types":0,
        "no-console":0,
        "react/no-find-dom-node":0,
        "react/no-deprecated":0
    }
}